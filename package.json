{
  "name": "@marketsquare/jupyterlab_robotmode",
  "version": "0.5.0",
  "description": "A JupyterLab extension which adds a CodeMirror mode for Robot Framework syntax",
  "author": "MarketSquare",
  "bugs": {
    "url": "https://github.com/MarketSquare/jupyterlab_robotmode/issues"
  },
  "dependencies": {
    "@codemirror/legacy-modes": "^6.3.3",
    "@jupyterlab/application": "^4.0.3",
    "@jupyterlab/codemirror": "^4.0.3",
    "@jupyterlab/filebrowser": "^4.0.3",
    "@jupyterlab/launcher": "^4.0.3",
    "@jupyterlab/mainmenu": "^4.0.3",
    "@jupyterlab/translation": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@jupyterlab/builder": "^4.0.3",
    "@types/codemirror": "^0.0.97",
    "@types/jest": "^27.4.1",
    "@types/jsdom": "^16.2.6",
    "@types/node": "^14.14.31",
    "babel-jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^27.1.4",
    "typescript": "~5.0.4",
    "yarn-berry-deduplicate": "^6.1.0"
  },
  "files": [
    "{lib,style}/**/*.{css,d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}"
  ],
  "homepage": "https://github.com/MarketSquare/jupyterlab_robotmode",
  "jupyterlab": {
    "extension": "lib/plugin.js",
    "outputDir": "jupyterlab_robotmode/_prefix/share/jupyter/labextensions/@marketsquare/jupyterlab_robotmode"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MarketSquare/jupyterlab_robotmode.git"
  },
  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc -b",
    "build:prod": "jlpm clean:all && jlpm build:lib && jlpm build:labextension",
    "clean": "jlpm clean:lib",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension",
    "clean:labextension": "rimraf jupyterlab_robotmode/_prefix",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "deduplicate": "yarn-berry-deduplicate -s fewer --fail || yarn-berry-deduplicate -s fewer --fail || yarn-berry-deduplicate -s fewer --fail",
    "install:extension": "jupyter labextension develop --overwrite .",
    "package": "npm pack",
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prettier:fix": "jlpm prettier:base --list-different --write",
    "prettier:base": "prettier \"*.{json,md,js,yml}\" \"{.github,src,tests,.binder}/**/*.{yml,json,ts,tsx,css,md,yaml}\"",
    "prettier:check": "jlpm prettier:base --check",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -b -w",
    "test": "jest"
  },
  "types": "lib/index.d.ts",
  "prettier": {
    "singleQuote": true,
    "proseWrap": "always",
    "printWidth": 88
  }
}
